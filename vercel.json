{
    "version": 2,
    "builds": [
      {
        "src": "dist/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "prisma/schema.prisma",
        "use": "@vercel/static-build",
        "config": { "command": "prisma generate" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/src/index.js"
      }
    ]
  }  